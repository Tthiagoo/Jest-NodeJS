{
  "name": "jestnodejs",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Tthiagoo <51219408+Tthiagoo@users.noreply.github.com>",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "dev": "nodemon --watch 'src/' --exec \"ts-node src/entry.ts\" -e ts",
    "test": "jest --no-cache"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/supertest": "^2.0.11",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6"
  }
}
